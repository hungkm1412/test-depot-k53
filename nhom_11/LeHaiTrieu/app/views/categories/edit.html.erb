<% provide(:title, 'New Category') %>
<h1>New Category</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@category) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.submit "Submit", class: "btn btn-large btn-primary" %>
      <% end %>
  </div>
</div>